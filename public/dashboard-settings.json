{
  "dashboard": {
    "title": "Platform Dashboard",
    "subtitle": "Project Issue Tracking"
  },
  "globalFilters": {
    "enabled": true,
    "availableFields": [
      {
        "name": "Type",
        "label": "Issue Type",
        "enabled": true
      },
      {
        "name": "Priority",
        "label": "Priority Level",
        "enabled": true
      },
      {
        "name": "State",
        "label": "Status",
        "enabled": true
      },
      {
        "name": "Assignee",
        "label": "Assigned To",
        "enabled": false
      }
    ],
    "allowMultiple": true,
    "defaultFilters": ["Type", "State"],
    "dateFilter": {
      "enabled": true,
      "defaultRange": "last-month"
    }
  },
  "charts": {
    "matrix": {
      "defaultXField": "State",
      "defaultYField": "Priority"
    }
  }
}
